# Super 分区逻辑卷
/system_root   erofs   /dev/block/bootdevice/by-name/system       flags=display=system;logical;slotselect;
/vendor        erofs   /dev/block/bootdevice/by-name/vendor       flags=display=vendor;logical;slotselect;
/product       ext4    /dev/block/bootdevice/by-name/product      flags=display=product;logical;slotselect;
/system_ext    erofs   /dev/block/bootdevice/by-name/system_ext   flags=display=system_ext;logical;slotselect;
/my_product    erofs   /dev/block/bootdevice/by-name/my_product   flags=display=my_product;logical;slotselect;
/my_engineering erofs  /dev/block/bootdevice/by-name/my_engineering flags=display=my_engineering;logical;slotselect;
/my_company    ext4    /dev/block/bootdevice/by-name/my_company   flags=display=my_company;logical;slotselect;
/my_carrier    erofs   /dev/block/bootdevice/by-name/my_carrier   flags=display=my_carrier;logical;slotselect;
/my_region     erofs   /dev/block/bootdevice/by-name/my_region    flags=display=my_region;logical;slotselect;
/my_heytap     erofs   /dev/block/bootdevice/by-name/my_heytap    flags=display=my_heytap;logical;slotselect;
/my_stock      erofs   /dev/block/bootdevice/by-name/my_stock     flags=display=my_stock;logical;slotselect;
/my_preload    ext4    /dev/block/bootdevice/by-name/my_preload   flags=display=my_preload;logical;slotselect;
/my_bigball    erofs   /dev/block/bootdevice/by-name/my_bigball   flags=display=my_bigball;logical;slotselect;
/my_manifest   erofs   /dev/block/bootdevice/by-name/my_manifest  flags=display=my_manifest;logical;slotselect;

# 物理分区
/cache         ext4    /dev/block/by-name/cache                   flags=display=cache
/metadata      ext4    /dev/block/by-name/metadata                flags=display=metadata
/data          f2fs    /dev/block/bootdevice/by-name/userdata     flags=display=data
/mnt/vendor/oplusreserve ext4 /dev/block/by-name/oplusreserve2    flags=display=oplusreserve
/special_preload ext4  /dev/block/by-name/preload                 flags=display=special_preload
/persist       ext4    /dev/block/bootdevice/by-name/persist      flags=display=persist
/opporeserve   ext4    /dev/block/bootdevice/by-name/oplusreserve2 flags=display=opporeserve

# 外部存储
/external_sd   vfat    /dev/block/mmcblk1p1                       flags=display=external_sd
/usb_otg       vfat    /dev/block/sdg1                            flags=display=usb_otg

# Boot/Recovery/Misc 等
/boot          emmc    /dev/block/bootdevice/by-name/boot         flags=display=boot
/recovery      emmc    /dev/block/bootdevice/by-name/recovery     flags=display=recovery
/misc          emmc    /dev/block/bootdevice/by-name/misc         flags=display=misc

# 其他 emmc 固件分区
/aboot         emmc    /dev/block/bootdevice/by-name/aboot        flags=display=aboot
/abootbak      emmc    /dev/block/bootdevice/by-name/abootbak     flags=display=abootbak
/modem         emmc    /dev/block/bootdevice/by-name/modem        flags=display=modem
/sbl1          emmc    /dev/block/bootdevice/by-name/sbl1         flags=display=sbl1
/sbl1bak       emmc    /dev/block/bootdevice/by-name/sbl1bak      flags=display=sbl1bak
/rpm           emmc    /dev/block/bootdevice/by-name/rpm          flags=display=rpm
/rpmbak        emmc    /dev/block/bootdevice/by-name/rpmbak       flags=display=rpmbak
/tz            emmc    /dev/block/bootdevice/by-name/tz           flags=display=tz
/tzbak         emmc    /dev/block/bootdevice/by-name/tzbak        flags=display=tzbak
/modemst1      emmc    /dev/block/bootdevice/by-name/modem_st1    flags=display=modemst1
/modemst2      emmc    /dev/block/bootdevice/by-name/modem_st2    flags=display=modemst2
/static_nv_bk  emmc    /dev/block/bootdevice/by-name/oppostanvbk  flags=display=static_nv_bk
/oppodycnvbk   emmc    /dev/block/bootdevice/by-name/oppodycnvbk  flags=display=oppodycnvbk
/logo          emmc    /dev/block/bootdevice/by-name/LOGO         flags=display=logo
/reserve4      emmc    /dev/block/bootdevice/by-name/reserve4     flags=display=reserve4
/dsp           emmc    /dev/block/bootdevice/by-name/dsp          flags=display=dsp
/DDR           emmc    /dev/block/bootdevice/by-name/DDR          flags=display=DDR
/devcfg        emmc    /dev/block/bootdevice/by-name/devcfg       flags=display=devcfg
/mdtp          emmc    /dev/block/bootdevice/by-name/mdtp         flags=display=mdtp
/cmnlib        emmc    /dev/block/bootdevice/by-name/cmnlib       flags=display=cmnlib
/cmnlibbak     emmc    /dev/block/bootdevice/by-name/cmnlibbak    flags=display=cmnlibbak
/keymaster     emmc    /dev/block/bootdevice/by-name/keymaster    flags=display=keymaster
/keymasterbak  emmc    /dev/block/bootdevice/by-name/keymasterbak flags=display=keymasterbak
/apdp          emmc    /dev/block/bootdevice/by-name/apdp         flags=display=apdp
/devinfo       emmc    /dev/block/bootdevice/by-name/devinfo      flags=display=devinfo
/abl           emmc    /dev/block/bootdevice/by-name/abl          flags=display=abl
/ablbak        emmc    /dev/block/bootdevice/by-name/ablbak       flags=display=ablbak
/xbl           emmc    /dev/block/bootdevice/by-name/xbl          flags=display=xbl
